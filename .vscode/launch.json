	"version": "0.2.0",
	"configurations": [
		{
			"name": "Django: runserver",
			"type": "python",
			"request": "launch",
			"preLaunchTask": "npm: dev",
			"program": "${workspaceFolder}\\forum\\manage.py",
			"args": [
				"runserver",
				"--noreload"
			],
			"django": true
		},
		{
			"name": "Django: make migrations",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}\\forum\\manage.py",
			"args": [
				"makemigrations"
			],
			"django": true
		},
		{
			"name": "Django: migrate",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}\\forum\\manage.py",
			"args": [
				"migrate"
			],
			"django": true
		}
	]
}